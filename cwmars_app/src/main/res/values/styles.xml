<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Missing material colors from https://material.google.com/style/color.html#color-color-palette -->
    <color name="material_grey_200">#EEEEEE</color>
    <color name="material_grey_400">#BDBDBD</color>
    <color name="material_grey_500">#9E9E9E</color>
    <color name="material_grey_700">#616161</color>

    <color name="cwmars_dark">#16416F</color>
    <color name="cwmars_medium">#006098</color>
    <color name="cwmars_secondary1">#d61d4a</color>
    <color name="cwmars_secondary2">#ee7623</color>
    <color name="cwmars_secondary3">#ffc629</color>
    <color name="cwmars_secondary4">#7bc24e</color>
    <color name="logo_background">@color/white</color>

    <color name="primary">@color/cwmars_dark</color>
    <color name="primary_dark">@color/primary_dark_material_dark</color>
    <color name="accent">@color/cwmars_secondary2</color>

    <style name="CWMARS" parent="Theme.AppCompat">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
    </style>

    <style name="CWMARSNoActionBar" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>
    </style>

    <style name="alertText">
        <item name="android:textColor">@color/cwmars_secondary4</item>
    </style>

    <style name="HemlockProgressBar" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="colorPrimaryDark">@color/accent</item>
        <item name="colorPrimary">@color/accent</item>
        <item name="colorAccent">@color/accent</item>
    </style>

</resources>
